#java-programs
import java.util.*;
public class reverse{
    public staticvoid main(string[]args){
        system.out.println("enter number:");
        int n=1234;
        int rev=0,rem;
        while(n!=0)
        {
            rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        }
        system.out.println("reverse="+rev);
        }
    }
