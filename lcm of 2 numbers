import java.until.*;
public class GCD {
    public static void main (string[]args) {
        system.out.println("enter 2 numbers:");
        int a=4;
        int b=6;
        int gcd=1;
        for(int i=1;i<=a&&i<=b;i++)
       {
           if(a%i==0&&b%i==0)
           gcd=i;
       }
       int lcm=(a*b)/gcd;
       system.out.println("lcm="+lcm);
    }
}
